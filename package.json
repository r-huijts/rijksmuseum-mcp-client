{
  "name": "electron-mcp-client",
  "version": "1.0.0",
  "description": "MCP Client for Rijksmuseum Art Collection",
  "main": "dist/main.js",
  "type": "module",
  "scripts": {
    "start": "tsc && node electron.cjs",
    "dev": "tsc -w",
    "build": "tsc",
    "dist": "electron-builder",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "build": {
    "appId": "com.electron.mcp-client",
    "mac": {
      "category": "public.app-category.utilities"
    },
    "files": [
      "dist/**/*",
      "node_modules/**/*"
    ]
  },
  "keywords": [
    "mcp",
    "electron",
    "rijksmuseum"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/electron": "^1.6.10",
    "@types/node": "^20.11.19",
    "electron": "^28.2.3",
    "electron-builder": "^24.9.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.33.1",
    "@modelcontextprotocol/sdk": "^1.0.0",
    "dotenv": "^16.4.1",
    "electron-is-dev": "^2.0.0",
    "zod": "^3.22.4"
  }
}
